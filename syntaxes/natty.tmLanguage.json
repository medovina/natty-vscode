{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Natty",
	"patterns": [
		{ "include": "#keywords" },
		{ "include": "#strings" }
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "entity.name.function.natty",
					"match": "^\\s*(\\d+|[a-z])\\."
				},
				{
					"name": "storage.type.natty",
					"match": "^\\s*(Axiom|Definition|Lemma|Proof|Theorem)( \\d*)?\\."
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.natty",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.natty",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.natty"
}
