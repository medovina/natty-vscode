{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Natty",
	"patterns": [
		{ "include": "#keywords" }
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "comment.line.number-sign.natty",
					"match": "#.*"
				},
				{
					"name": "entity.name.function.natty",
					"match": "^\\s*(\\d+|[a-z])\\."
				},
				{
					"name": "storage.type.natty",
					"match": "^\\s*(Axiom|Definition|Lemma|Proof|Theorem)( \\d*)?\\."
				}
			]
		}
	},
	"scopeName": "source.natty"
}
